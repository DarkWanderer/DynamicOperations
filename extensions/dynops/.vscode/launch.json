        {
            "name": "Cluster Test Stratis",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/dynops-test.exe",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "${workspaceFolder}\\data\\objects.Stratis.txt"
            ],
            "environment": [
                {
                    "name": "RUST_BACKTRACE",
                    "value": "1"
                }
            ],
            "console": "integratedTerminal",
